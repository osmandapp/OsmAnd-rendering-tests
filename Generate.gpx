<?xml version='1.0' encoding='UTF-8' standalone='yes' ?>
<gpx version="1.1" creator="OsmAnd~" xmlns="http://www.topografix.com/GPX/1/1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.topografix.com/GPX/1/1 http://www.topografix.com/GPX/1/1/gpx.xsd" width="1366" height="768" zoomScale="0" zoom="13" renderingProperties="hideBuildings=false;nightMode=true" renderingName="default">
  <extensions>
      <html>                    
      <head>                   
        <title>Rendering test results in HTML</title>                    
      </head>                                              
      <body>                   
      <!-- div class="image-gallery" -->   
      <table>
        <tr>
            <th>Default</th>            
            <th>Default-night</th>
        </tr>
        <ROW>
        <tr>
          <ROW_ELEMENT>
          <td>
                <a href="$PWD/$FILENAME">
                  <img src="$PWD/$FILENAME" width="444" height="250" alt=""/></a>
          </td>
          </ROW_ELEMENT>
        </tr>
        <tr>
            <td colspan="4"><p>$DESCRIPTION</p></td>        
        </tr>
        </ROW>                    
      </table>
      </body>                    
      </html> 
  </extensions>
  <wpt lat="46.0621687" lon="7.3562649">
    <name>Glaciers</name>
    <desc>Tests several adjacent glaciers both day and night view</desc>
    <extensions>
    	<maps>Switzerland_europe_2</maps>
	</extensions>
    <category>Rendering-test</category>
  </wpt>

  <wpt lat="49.027662" lon="12.0953237">
    <name>Bridges</name>
